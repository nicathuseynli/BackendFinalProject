@model CreateAboutUsVM
<div class="container">
    <h1 class="text-center">Create About Team</h1>
      <div class="row">
             <form asp-action="Create" method="post" class="forms-sample" enctype="multipart/form-data">
                 <div class="form-group">
                   <input asp-for="Information" class="form-control" placeholder="Information">
                 </div><div class="form-group">
                   <input asp-for="VideoLink" class="form-control" placeholder="Videolink">
                 </div>
                 <div class="form-group">
                   <input asp-for="CenterTitle" class="form-control"placeholder="Centertitle">
                 </div>  
                 <div class="form-group">
                   <label>About Image</label>
                   <input asp-for="Photo" class="form-control">
                 </div>
                 <button type="submit" class="btn btn-outline-success me-2">Create</button>
                 <button asp-action="Index" class="btn btn-outline-danger">Cancel</button>
             </form>
    </div>
</div>
